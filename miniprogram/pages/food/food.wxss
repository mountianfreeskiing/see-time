.food-page {
  width: 100%;
  height: 100%;
}
.food-page .page-title {
  color: #000000;
  font-size: 54rpx;
  font-weight: 600;
  margin-left: 44rpx;
}
.food-page .input-wrap {
  min-height: 115rpx;
  position: relative;
  border: 6rpx solid transparent;
  margin: 34rpx 46rpx;
  background: #ffffff;
  box-shadow: 0px 12rpx 0px 0px #ffd618;
  border-radius: 43rpx;
  display: flex;
  align-items: center;
}
.food-page .input-wrap .food-input-holder {
  color: #F9D569;
  font-size: 36rpx;
  font-weight: 400;
}
.food-page .input-wrap .food-input {
  width: 100%;
  color: #686766;
  font-size: 36rpx;
  font-weight: 400;
  caret-color: #F9D569;
  line-height: 55rpx;
}
.food-page .input-wrap .food-input .ingredient-list {
  display: inline-flex;
  align-items: center;
  flex-direction: row;
  justify-content: center;
  margin: 10rpx 10rpx;
}
.food-page .input-wrap .food-input .ingredient-list .every-ingredient {
  color: #686766;
  font-size: 36rpx;
  font-weight: 400;
  max-width: 200rpx;
  min-height: 55rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  align-items: center;
  z-index: 1;
}
.food-page .input-wrap .food-input .ingredient-list .every-ingredient-select {
  color: #686766;
  font-size: 36rpx;
  font-weight: 400;
  max-width: 200rpx;
  min-height: 55rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  align-items: center;
  z-index: 1;
  border: 3rpx solid hsl(49, 100%, 55%);
  border-radius: 24rpx;
  padding: 0 22rpx;
  z-index: 2;
  box-shadow: 5rpx 5rpx 5rpx #F9D569;
}
.food-page .input-wrap::after {
  content: "";
  display: block;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
  bottom: -20rpx;
  left: -10rpx;
  border-radius: 43rpx;
  background: linear-gradient(0deg, #ffc618, #e8c6fd);
  z-index: -1;
}
