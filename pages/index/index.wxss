.index-container {
  display: grid;
  margin-top: 10rpx;
  grid-template-columns: repeat(auto-fill, 100px);
  grid-template-rows: 100px;
  grid-auto-rows: 100px;
  justify-content: center;
  grid-gap: 10rpx;
  padding: 10rpx;
}
.index-container .index-entrance {
  font-size: 40rpx;
  border: 1rpx solid #e5e4e9;
  display: flex;
  align-items: center;
  justify-content: center;
}
